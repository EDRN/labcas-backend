<!-- XML snippet containing configuration for LabCAS Common Data Elements (CDEs) -->

<configuration>

    <!--  UI workflow order -->
    <property name="workflow.order" value="1" />
    <property name="UploadFiles" value="true" />
    
    <!-- Collection level metadata -->    
    
    <property name="input.dataset.CollectionName.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.dataset.CollectionName.title" value="Data Collection Name" />
    <property name="input.dataset.CollectionName.description" value="Short name for this data collection (one or a few words only)." />
    <property name="input.dataset.CollectionName.required" value="true" />
    
    <property name="input.dataset.CollectionDescription.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#text" />
    <property name="input.dataset.CollectionDescription.title" value="Data Collection Description" />
    <property name="input.dataset.CollectionDescription.description" value="Short description for this data collection (a few sentences)." />
    <property name="input.dataset.CollectionDescription.required" value="true" />
    
    <property name="input.dataset.Discipline.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#discipline" />
    <property name="input.dataset.Discipline.title" value="Discipline" />
    <property name="input.dataset.Discipline.description" value="Medical discipline for this dataset (select one)" />
    <property name="input.dataset.Discipline.required" value="false" />
    
    <property name="input.dataset.Institution.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#siteName" />
    <property name="input.dataset.Institution.title" value="Institution" />
    <property name="input.dataset.Institution.description" value="Institution where the research was conducted (full name)" />
    <property name="input.dataset.Institution.required" value="true" />
    
    <property name="input.dataset.InstitutionId.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#siteNameId" />
    <property name="input.dataset.InstitutionId.title" value="Institution Unique Identifier" />
    <property name="input.dataset.InstitutionId.description" value="Institution Unique Identifier" />
    <property name="input.dataset.InstitutionId.required" value="false" />
    <property name="input.dataset.InstitutionId.visible" value="false" />
    
    <property name="input.dataset.LeadPI.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#principalInvestigator" />
    <property name="input.dataset.LeadPI.title" value="Lead PI" />
    <property name="input.dataset.LeadPI.description" value="Begin typing an investigator name. A list of options will appear." />
    <property name="input.dataset.LeadPI.required" value="true" />

    <property name="input.dataset.LeadPIId.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#principalInvestigator" />
    <property name="input.dataset.LeadPIId.title" value="Lead PI Identifier" />
    <property name="input.dataset.LeadPIId.description" value="Lead PI Unique Identifier" />
    <property name="input.dataset.LeadPIId.required" value="false" />
    <property name="input.dataset.LeadPIId.visible" value="false" />
    
    <property name="input.dataset.Organ.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#organSite" />
    <property name="input.dataset.Organ.title" value="Organ" />
    <property name="input.dataset.Organ.description" value="Organ (select all that apply)" />
    <property name="input.dataset.Organ.required" value="true" />

    <property name="input.dataset.OrganId.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#organSiteId" />
    <property name="input.dataset.OrganId.title" value="Organ Identifier" />
    <property name="input.dataset.OrganId.description" value="Organ Unique Identifier" />
    <property name="input.dataset.OrganId.required" value="true" />
    <property name="input.dataset.OrganId.visible" value="false" />
    
    <property name="input.dataset.ProtocolName.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#protocolName" />
    <property name="input.dataset.ProtocolName.title" value="Protocol Name" />
    <property name="input.dataset.ProtocolName.description" 
              value="Begin typing a protocol name or id. A list of options will appear. Don’t see the protocol you want? Email edrn-ic@jpl.nasa.gov" />
    <property name="input.dataset.ProtocolName.required" value="true" />
    
    <property name="input.dataset.ProtocolId.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#protocolId" />
    <property name="input.dataset.ProtocolId.title" value="Protocol Identifier" />
    <property name="input.dataset.ProtocolId.description" value="Protocol Unique Identifier" />
    <property name="input.dataset.ProtocolId.required" value="false" />
    
    <property name="input.dataset.DataCustodian.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.dataset.DataCustodian.title" value="Data Custodian" />
    <property name="input.dataset.DataCustodian.description" value="Name of the dataset custodian in 'Last, First' format" />
    <property name="input.dataset.DataCustodian.required" value="true" />
    
    <property name="input.dataset.OwnerPrincipal.type" value="urn:ldap:attributes:dn" />
    <property name="input.dataset.OwnerPrincipal.title" value="Share data with..." />
    <property name="input.dataset.OwnerPrincipal.description" 
              value="Select or remove the groups or individuals with whom you’d like to share this data." />
    <property name="input.dataset.OwnerPrincipal.required" value="true" />
    <property name="input.dataset.OwnerPrincipal.visible" value="true" />
    
    <property name="input.dataset.CollaborativeGroup.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#collaborativeGroup" />
    <property name="input.dataset.CollaborativeGroup.title" value="Collaborative Group" />
    <property name="input.dataset.CollaborativeGroup.description" value="Organ Collaborative Group (select one)" />
    <property name="input.dataset.CollaborativeGroup.required" value="true" />
    
    <property name="input.dataset.Consortium.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.dataset.Consortium.title" value="Consortium" />
    <property name="input.dataset.Consortium.description" value="High level project funding these data" />
    <property name="input.dataset.Consortium.required" value="true" />
    <property name="input.dataset.Consortium.value.1" value="EDRN" />
    <property name="input.dataset.Consortium.value.2" value="MCL" />
    <property name="input.dataset.Consortium.visible" value="false" />
    
  
    <!--  Dataset level metadata -->
    
    <property name="input.dataset.DatasetName.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.dataset.DatasetName.title" value="Dataset Name" />
    <property name="input.dataset.DatasetName.description" value="Short name for this dataset (one or a few words only)." />
    <property name="input.dataset.DatasetId.required" value="true" />
    
    <property name="input.dataset.DatasetDescription.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#text" />
    <property name="input.dataset.DatasetDescription.title" value="Dataset Description" />
    <property name="input.dataset.DatasetDescription.description" value="Short description for this dataset (a few sentences)." />
    <property name="input.dataset.DatasetDescription.required" value="true" />
    
    <property name="input.dataset.Species.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#species" />
    <property name="input.dataset.Species.title" value="Species" />
    <property name="input.dataset.Species.description" value="Animal species for which this study was conducted" />
    <property name="input.dataset.Species.required" value="true" />
    
        
    <property name="input.dataset.PubMedId.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#discipline" />
    <property name="input.dataset.PubMedId.title" value="Pub Med ID" />
    <property name="input.dataset.PubMedId.description" value="URL of PubMed publication" />
    <property name="input.dataset.PubMedId.required" value="false" />
        
    <property name="input.dataset.ReferenceURL.type" value="http://www.w3.org/2001/XMLSchema/url" />
    <property name="input.dataset.ReferenceURL.title" value="Reference URL" />
    <property name="input.dataset.ReferenceURL.description" value="External URL of associated web site for these data" />
    <property name="input.dataset.ReferenceURL.required" value="false" />
        
    <property name="input.file.Instrument.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.Instrument.title" value="Instrument" />
    <property name="input.file.Instrument.description" value="Instrument that was used to collect these data" />
    <property name="input.file.Instrument.required" value="false" />
    
    <property name="input.file.SpecimenType.type" value="http://edrn.nci.nih.gov/xml/schema/types.xml#specimenType" />
    <property name="input.file.SpecimenType.title" value="Specimen Type" />
    <property name="input.file.SpecimenType.description" value="The specimen type (select all that apply)" />
    <property name="input.file.SpecimenType.required" value="false" />   
        
    <!-- File level metadata -->
    
    <property name="input.file.FileName.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.FileName.title" value="File Name" />
    <property name="input.file.FileName.description" value="The file name on storage" />
    <property name="input.dataset.FileName.required" value="true" />

    <property name="input.file.FilePath.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.FilePath.title" value="File Path" />
    <property name="input.file.FilePath.description" value="The file path on storage" />
    <property name="input.dataset.FilePath.required" value="true" />

    <property name="input.file.FileType.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.FileType.title" value="File Type" />
    <property name="input.file.FileType.description" value="The file mime type" />
    <property name="input.dataset.FileType.required" value="false" />

    <property name="input.file.FileDescription.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.FileDescription.title" value="File Name" />
    <property name="input.file.FileDescription.description" value="A short description for this file" />
    <property name="input.dataset.FileDescription.required" value="false" />

    <property name="input.file.ProcessingLevel.type" value="http://www.w3.org/2001/XMLSchema/string" />
    <property name="input.file.ProcessingLevel.title" value="Raw or Processed" />
    <property name="input.file.ProcessingLevel.description" value="Wether this file contains raw or processed data" />
    <property name="input.dataset.ProcessingLevel.required" value="false" />
    
      
    <!-- output products -->
    
    <property name="output.product.type" value="urn:edrn:LabCAS_Product"/>
      
</configuration>