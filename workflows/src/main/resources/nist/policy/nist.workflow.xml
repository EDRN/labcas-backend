<cas:workflow xmlns:cas="http://oodt.jpl.nasa.gov/1.0/cas" name="NIST dataset" id="urn:edrn:NistWorkflow">

  <tasks>
  
    <!-- initializes product type, version -->
    <task id="urn:edrn:NistInitTask" />
        
    <!-- runs R script -->
    <task id="urn:edrn:NistExecTask" /> 
    
    <!-- crawls the staging area and publishes the files -->
    <task id="urn:edrn:NistCrawlTask" />
    
  </tasks>

</cas:workflow>
