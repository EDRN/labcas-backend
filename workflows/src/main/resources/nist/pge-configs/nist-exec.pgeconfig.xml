<?xml version="1.0" encoding="UTF-8"?>
<pgeConfig>

  <!-- PGE commands -->
  <exe dir="[JobDir]" shell="/bin/bash">
    <cmd>cd [LABCAS_HOME]/workflows/nist/pges</cmd>
    <cmd>Rscript ./twoaxes.r</cmd>
    <cmd>cp Rplots.pdf [LABCAS_STAGING]/[DatasetId]/Rplots.pdf</cmd>
  </exe>
  
  <customMetadata>
    <metadata key="ProductionDateTime" val="[DATE.UTC]" />
    <metadata key="JobDir" val="[PGE_ROOT]/nist/jobs/[DatasetId]" />
  </customMetadata>
  
</pgeConfig>